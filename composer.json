{
    "name": "fabpot/silex-skeleton",
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "~1.0@dev",
        "twig/twig": ">=1.8.0,<2.0-dev",
        "monolog/monolog": "@stable",
        "symfony/symfony": "~2.2@dev",
        "doctrine/dbal": "*",
        "guzzle/guzzle": "*",
        "beberlei/tent-php": "dev-master",
        "ruudk/twitter-oauth": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "silex/web-profiler": "~1.0@dev",
        "phpunit/phpunit": "3.7.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ]
}
